{
    "name": "band-manager",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "postinstall": "git -v > /dev/null && git config --local core.hooksPath ./script/git-hooks || true",
        "dev": "next dev",
        "dev:turbo": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "fmt": "prettier --write ./src",
        "lint": "next lint && tsc --noEmit",
        "lint:fix": "next lint --fix",
        "cs": "pnpm lint:fix && pnpm fmt",
        "migrate": "node --no-warnings --loader ts-node/esm script/migrate.ts",
        "create-migration": "node ./script/createMigration.mjs",
        "password:hash": "node --no-warnings --loader ts-node/esm script/argon.ts --hash",
        "password:verify": "node --no-warnings --loader ts-node/esm script/argon.ts --verify"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.17",
        "@planetscale/database": "^1.11.0",
        "@t3-oss/env-nextjs": "^0.3.1",
        "@tanstack/react-query": "^4.35.3",
        "@trpc/client": "^10.38.4",
        "@trpc/react-query": "^10.38.4",
        "@trpc/server": "^10.38.4",
        "argon2": "^0.31.1",
        "connection-string": "^4.4.0",
        "dayjs": "^1.11.10",
        "jose": "^4.14.6",
        "kysely": "^0.24.2",
        "kysely-planetscale": "^1.4.0",
        "next": "^13.5.2",
        "react": "^18.2.0",
        "react-colorful": "^5.6.1",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-icons": "^4.11.0",
        "sharp": "^0.32.6",
        "sprintf-js": "^1.1.3",
        "tailwind-merge": "^1.14.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@tanstack/react-query-devtools": "^4.35.3",
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/node": "20.1.0",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/sprintf-js": "^1.1.2",
        "autoprefixer": "10.4.14",
        "dotenv": "^16.3.1",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.1",
        "eslint-plugin-unused-imports": "^3.0.0",
        "mysql2": "^3.6.1",
        "postcss": "8.4.23",
        "prettier": "^2.8.8",
        "tailwindcss": "3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "5.0.4"
    }
}